# AWS region
aws_region = "us-east-1"

# Availability zone (must match region)
availability_zone = "us-east-1a"

# AMI ID for your EC2 instance (example is Amazon Linux 2)
ami_id = "ami-0e58b56aa4d64231b" # Replace with a valid AMI ID for your region

# Instance type for EC2
instance_type = "t2.micro"
